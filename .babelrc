{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@assets": "./src/assets",
          "@type": "./src/types",
          "@stores": "./src/stores",
          "@styles": "./src/styles",
          "@components": "./src/components",
          "@screens": "./src/screens"
        }
      }
    ]
  ]
}
